{
  "executable": "AIDemos",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "AIDemos",
  "author": "Bill SerGio, The Infomercial King",
  "singleInstance": true,
  "environment": "Production",
  "build": {
    "appId": "com.AIDemos.app",
    "productName": "AIDemos",
    "copyright": "Copyright Â© 2020 - 2024",
    "buildVersion": "3.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}